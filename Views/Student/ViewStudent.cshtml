@model Student_Register.Models.StudentArpit

@{
    ViewData["Title"] = "ViewStudent";
}

<h1>Student Details</h1>

<div>
    
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @* @Html.DisplayNameFor(model => model.Rollno) *@
            Roll No.
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Rollno)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Name)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt class = "col-sm-2">
            @* @Html.DisplayNameFor(model => model.Dob) *@
            Date of Birth
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Dob)
        </dd>
        <dt class = "col-sm-2">
            @* @Html.DisplayNameFor(model => model.Cid) *@
            Course
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Cid)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Edit", "EditStudent", new { id = Model.Rollno }) |
    <a asp-action="GetStudents">Back to List</a>
</div>
